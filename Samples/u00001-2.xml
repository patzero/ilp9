<program>
   <!-- test:name description='operateur binaire - (resultat negatif)' -->
   <ternaryOperation operator="?:">
   
	<!-- result = 0 > 1 ? true : false; -->
	
	  <!-- if 0 > 1-->
      <firstOperand>
        <binaryOperation operator="&gt;">
		  <leftOperand>
			 <integer value="0" />
		  </leftOperand>
		  <rightOperand>
			 <integer value="1" />
		  </rightOperand>
	    </binaryOperation>
      </firstOperand>
	  
	  <!-- then value = true -->
      <secondOperand>
		 <boolean value="true" />
      </secondOperand>
	  
	  <!-- else value = false-->
	  <thirdOperand>
         <boolean value="false" />
      </thirdOperand>
	  
   </ternaryOperation>
</program>
